{
  "name": "circleci-logs",
  "version": "0.0.5",
  "description": "Fetch CircleCI job step logs from a gh pr checks URL",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "circleci-logs": "./dist/index.js"
  },
  "man": "./man/circleci-logs.1",
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "tsx scripts/add-shebang.ts",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src",
    "format": "prettier --write 'src/**/*.ts' 'scripts/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'scripts/**/*.ts'",
    "generate-docs": "tsx scripts/generate-docs.ts",
    "prebuild": "pnpm run generate-docs",
    "prepublishOnly": "pnpm run format:check && pnpm run lint && pnpm run typecheck && pnpm test && pnpm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "circleci",
    "ci",
    "logs",
    "cli",
    "github",
    "pr",
    "checks"
  ],
  "author": "mkusaka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkusaka/circleci-logs.git"
  },
  "bugs": {
    "url": "https://github.com/mkusaka/circleci-logs/issues"
  },
  "homepage": "https://github.com/mkusaka/circleci-logs#readme",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.map",
    "!dist/**/*.test.*",
    "man",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "marked-man": "^2.1.0",
    "msw": "^2.10.5",
    "oxlint": "^1.12.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "chalk": "^5.6.0",
    "commander": "^14.0.0"
  }
}
