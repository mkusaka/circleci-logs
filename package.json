{
  "name": "circleci-logs",
  "version": "0.0.0",
  "description": "Fetch CircleCI job step logs from a gh pr checks URL",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "circleci-logs": "./dist/index.js"
  },
  "man": "./man/circleci-logs.1",
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src",
    "format": "prettier --write src/**/*.ts",
    "generate-docs": "tsx scripts/generate-docs.ts",
    "prebuild": "pnpm run generate-docs"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "marked-man": "^2.1.0",
    "oxlint": "^1.12.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "chalk": "^5.6.0",
    "commander": "^14.0.0"
  }
}
